{
    "env": {
        "browser": false ,
        "node":true, 
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended", 
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint", 
        "prettier"
    ],
    "rules": {
      "no-console":1,
      "no-debugger":0,
      "no-dupe-keys":2,
      "semi":1,
      "no-empty":2,
      "no-useless-catch":1,
      "no-useless-return":1,
      "no-extra-label":2,
      "no-extra-semi":0,
      "no-var":2
    }
}
